{
	"$id": "https://wp-fastendpoints/schemas/Users/Small.json",
	"type": "object",
	"properties": {
		"data": {
			"type": "object",
			"properties": {
				"ID": {
					"type": "integer"
				},
				"user_email": {
					"type": "string",
					"format": "email",
					"maxLength": 50
				}
			},
			"required": [
				"ID",
				"user_email"
			]
		},
		"is_admin": {
			"type": "boolean",
			"default": false
		}
	},
	"required": [
		"data",
		"is_admin"
	]
}