{
	"$id": "https://wp-fastendpoints/schemas/Users/WithAdditionalProperties.json",
	"type": "object",
	"properties": {
		"data": {
			"type": "object",
			"properties": {
				"user_email": {
					"type": "string",
					"format": "email",
					"maxLength": 50
				},
				"user_url": {
					"type": "string",
					"pattern": "(https?:\/\/(?:www\\.|(?!www))[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|www\\.[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|https?:\/\/(?:www\\.|(?!www))[a-zA-Z0-9]+\\.[^\\s]{2,}|www\\.[a-zA-Z0-9]+\\.[^\\s]{2,})"
				},
				"display_name": {
					"type": "string",
					"maxLength": 100
				}
			},
			"additionalProperties": false,
			"required": [
				"user_email"
			]
		}
	},
	"additionalProperties": {
		"type": "string"
	},
	"required": [
		"data"
	]
}
